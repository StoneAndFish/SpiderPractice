        $(function () {
            // Not using is:visible checks in case people click multiple times
            var isToggled = false;

            $('.advanced-tips-toggle').on("click", function() {
                if (!isToggled) {
                    $('.advanced-tips').slideDown(100, function () {
                        $('.advanced-tips table').fadeTo(200, 1);
                    });
                } else {
                    $('.advanced-tips table').fadeTo(200, 0, function () {
                        $('.advanced-tips').slideUp(100);
                    });
                }
                isToggled = !isToggled;
                return false;
            });
        });
